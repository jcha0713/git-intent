{
  "name": "git-intent",
  "version": "0.0.0",
  "description": "Git workflow tool for intentional commits - Define your commit intentions before coding to write more meaningful and purposeful code",
  "keywords": [
    "git",
    "intentional-commits",
    "commit",
    "git-workflow",
    "meaningful-commits",
    "commit-first",
    "git-todo",
    "todo-driven"
  ],
  "license": "MIT",
  "files": ["dist"],
  "type": "module",
  "exports": "./dist/index.js",
  "bin": "./dist/index.js",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "start": "node dist/index.js",
    "format": "biome format",
    "format:fix": "biome format --write"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "execa": "^9.5.2",
    "fs-extra": "^11.3.0",
    "inquirer": "^12.4.1",
    "nanoid": "^5.0.9",
    "ora": "^8.2.0",
    "simple-git": "^3.27.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.13.1",
    "@types/nodegit": "^0.28.7",
    "tsup": "^8.3.6",
    "type-fest": "^4.34.1",
    "typescript": "^5.7.3",
    "zod": "^3.24.2"
  },
  "packageManager": "pnpm@10.3.0"
}
